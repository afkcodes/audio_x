{
  "name": "audio_x",
  "version": "1.0.12",
  "description": "The audio player for the gen-x",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
