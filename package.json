  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/config-conventional": "^19.8.0",
    "@release-it/conventional-changelog": "^10.0.0",
    "@swc/core": "^1.11.21",
    "@types/node": "^22.14.1",
    "commitlint": "^19.8.0",
    "lefthook": "^1.11.10",
    "release-it": "^18.1.2",
    "rimraf": "^6.0.1",
